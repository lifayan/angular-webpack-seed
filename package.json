{
  "name": "angular-webpack-seed",
  "version": "0.1.0",
  "description": "angualr project template",
  "main": "index.js",
  "scripts": {
    "build": "babel-node --presets es2015 -- ./node_modules/webpack/bin/webpack.js --config webpack-production.config.js --progress --profile --colors",
    "build-dev": "babel-node --presets es2015 -- ./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress --profile --colors",
    "dev-server": "babel-node --presets es2015 -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack-dev-server.config.js --progress --colors --port 8080 --inline --content-base public",
    "test": "babel-node --presets es2015 -- ./node_modules/karma-cli/bin/karma start",
    "cover": "rimraf coverage && COVERAGE=1 babel-node --presets es2015 -- ./node_modules/karma-cli/bin/karma start",
    "lint": "eslint ./src/"
  },
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "^1.4.1",
    "angular-chart.js": "^1.0.0-alpha6",
    "angular-ui-bootstrap": "^1.3.3",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.1",
    "autoprefixer": "^6.1.1",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-istanbul-loader": "0.0.3",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-runtime": "^6.1.18",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.11.6",
    "css-loader": "^0.23.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "loader-utils": "^0.2.12",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "node-sass": "^3.2.0",
    "postcss-loader": "^0.8.0",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.0.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  }
